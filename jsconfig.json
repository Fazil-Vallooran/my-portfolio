{
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": ["./src/*"],
      "@icons/*": ["Icons/*"],
      "@components/*": ["Components/*"],
      "@layout/*": ["Components/layout/*"],
      "@constants/*": ["constants/*"],
      "@contexts/*": ["contexts/*"],
      "@ui/*": ["Components/ui/*"],
      "@sections/*": ["sections/*"],
      "@util/*": ["util/*"]
    }
  },
  "exclude": ["node_modules"]
}
